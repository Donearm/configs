VERSION: 3.x.x
BBCDWRITER: /dev/cdrom
BBCDROM: /dev/cdrom1
BBCDMNT: /dev/cdrom1
BBSPEED: -1
BBBLANKING: fast
BBNUMDEV: 2
BBBURNDIR: /mnt/documents/burn/
BBLABEL: <ask-me>
BBDESCRIPTION: Burnt with BashBurn
BBAUTHOR: Gianluca Fiore
BBNORMALIZE: no
BBDRIVEROPT:
BBFIFODIR: /tmp
BBDELTEMPBURN: no
BBOVERBURN: yes
BBBITRATE: 256
BBLANG: English
BBISCONF: 0
BBDTAO: -tao
BBPADDING: -nopad
BBCONFFILE: /home/gianluca//.bashburnrc
BB_CDBURNCMD: cdrecord
BB_DVDBURNCMD: growisofs
BB_DVDBURNCMDOPTS: -r -f -v -J -joliet-long
BB_ISOCMD: mkisofs
BB_DVDBLANK: dvd+rw-format
BB_CDIMAGECMD: cdrdao
BB_CDAUDIORIP: cdparanoia
BB_READ_OPTS: -r -R -J -l --allow-leading-dots
BB_READCD: mkisofs
BB_MP3ENC: lame
BB_MP3DEC: mpg123
BB_OGGENC: oggenc
BB_OGGDEC: oggdec
BB_FLACCMD: flac
BB_EJECT: eject
BB_NORMCMD: normalize
