#
# System configuration file for Mutt
#

# default list of header fields to weed when displaying
#
#ignore "from " received content- mime-version status x-status message-id
#ignore sender references return-path lines
ignore *
unignore From: Date: To: CC: BCC: Subject: Reply-To: Content-Type: \
Organization: X-Organization: X-Operating-System: X-Mailer: User-Agent: \
X-Disclaimer: X-Editor: X-Face: X-Spam-Status: X-Spam-Level: Message-ID:

# imitate the old search-body function
macro index \eb '/~b ' 'search in message bodies'

# use Tab and Esc-Tab to move from an Old message to a New or Old one
# instead than from New to New
bind pager \t	next-unread
bind index \t	next-unread
bind pager \e\t	    previous-unread
bind index \e\t	    previous-unread

# crypt with rot13
macro pager \cr "|formail -I '' | tr a-zA-Z n-za-mN-ZA-M | less -c\n" "ROT_13"

# simulate the old url menu
macro index \cb |urlview\n 'call urlview to extract URLs out of a message'
macro pager \cb |urlview\n 'call urlview to extract URLs out of a message'
#
# call fetchmail
macro generic G <shell-escape>'fetchmail'<enter> 'Run fetchmail' 

# Show documentation when pressing F1
macro generic <f1> "!less /usr/share/doc/mutt-1.4.1i/manual.txt\n" "Show Mutt documentation"
macro index   <f1> "!less /usr/share/doc/mutt-1.4.1i/manual.txt\n" "Show Mutt documentation"
macro pager   <f1> "!less /usr/share/doc/mutt-1.4.1i/manual.txt\n" "Show Mutt documentation"

