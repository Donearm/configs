#!/bin/sh

# Load some defaults for each user
source /etc/X11/xinit/xinitrc.d/50-systemd-user.sh

#display -window root /mnt/documents/d/Images/French_Polynesia.jpg &
feh --bg-fill ~/documents/Images/bali_rice_terraces.jpg &

xbindkeys
setxkbmap -model pc105 -layout us -variant altgr-intl -option compose:menu
xset m 0.7 1.6
xset dpms 0 120 600
xrandr --dpi 96
lxsession &
parcellite &
zettlr &
firefox &
kitty &
kitty &

exit 0

# vi: ft=sh:
